<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <color name="pressedColor">#cc33B5E5</color>
    <color name="focusedColor">#6633B5E5</color>
    <color name="selectedColor">#3333B5E5</color>
    <color name="popupMessageBgColor">#88000000</color>
    <color name="popupMessageTextColor">#fff3f3f3</color>
    
    <color name="timetableBorderColor">#ffbbbbbb</color>
    <color name="timeBarColor">#ffD6ADEB</color>
    <color name="broadcastTimeBarColor">#ff8AD5F0</color>
    
    <dimen name="timeBarHeight">6dip</dimen>
    
    <color name="chNameColor">#ff008800</color>
    <color name="timeColor">#ff333333</color>
    <color name="titleColor">#ff000000</color>
    <color name="nowStartedBackgroundColor">#ffFFFFBB</color>
    
    <style name="popupMessage">
        <item name="android:background">@drawable/popup_message_bg</item>
        <item name="android:text">@color/popupMessageTextColor</item>
    </style>

    <style name="keywordEditStyle">
        <item name="android:hint">@string/keywordHint</item>
        <item name="android:singleLine">true</item>
        <item name="android:inputType">text</item>
    </style>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <color name="windowBackgroundColor">#fff3f3f3</color>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">

        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowBackground">@color/windowBackgroundColor</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

</resources>